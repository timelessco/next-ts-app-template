{
	"name": "timelessco-nextjs",
	"version": "0.1.0",
	"private": true,
	"description": "Timeless Co Portfolio",
	"keywords": [
		"react",
		"typescript",
		"nextjs",
		"tailwindcss",
		"portfolio",
		"timelessco",
		"timeless",
		"timelessco-nextjs",
		"timelessco-nextjs-portfolio"
	],
	"bugs": {
		"url": "https://github.com/timelessco/timelessco-nextjs/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/timelessco/timelessco-nextjs.git"
	},
	"license": "MIT",
	"author": {
		"name": "Timeless",
		"email": "hello@timeless.co",
		"url": "https://timeless.co/"
	},
	"type": "module",
	"scripts": {
		"build": "next build",
		"build:analyze": "cross-env ANALYZE=true pnpm build && pnpm start",
		"build:ci": "cross-env SKIP_ENV_VALIDATION=true pnpm build",
		"build:icons": "node --experimental-strip-types scripts/build-icons.ts",
		"build:local": "cross-env LOCAL=true pnpm build",
		"build:sourcemap": "cross-env SOURCEMAP=true pnpm build && source-map-explorer .next/static/**/*.js && pnpm start",
		"build:start": "pnpm build && pnpm start",
		"check:engine": "npx installed-check --fix",
		"check:packages": "pnpm dedupe --check",
		"check:update": "npx npm-check-updates --interactive --format group --target patch",
		"clean": "node --experimental-strip-types scripts/clean.ts",
		"contributors:add": "all-contributors add",
		"contributors:generate": "all-contributors generate",
		"dev": "cross-env SERWIST_SUPPRESS_TURBOPACK_WARNING=1 next dev --turbopack",
		"dev:sourcemap": "cross-env SOURCEMAP=true pnpm dev",
		"fix": "turbo fix:eslint",
		"fix:css": "stylelint --fix --cache --ignore-path .gitignore --color -f verbose '**/*.css'",
		"fix:eslint": "eslint --cache --color --fix .",
		"fix:md": "markdownlint-cli2 --fix --rules sentences-per-line",
		"fix:prettier": "prettier --write --cache --cache-location=.prettiercache --list-different --ignore-unknown .",
		"fix:spelling": "node --experimental-strip-types scripts/update-cspell-words.ts",
		"lint": "turbo lint:prettier lint:eslint lint:types lint:knip lint:md lint:spelling lint:css",
		"lint:css": "stylelint --cache --ignore-path .gitignore --color -f verbose '**/*.css'",
		"lint:eslint": "eslint --cache --color .",
		"lint:knip": "knip --production",
		"lint:md": "markdownlint-cli2 --rules sentences-per-line",
		"lint:prettier": "prettier --check --cache --cache-location=.prettiercache --ignore-unknown .",
		"lint:spelling": "cspell --dot --gitignore --cache --no-progress --show-context check \"**/*\"",
		"lint:types": "tsc --noEmit",
		"prepare": "is-ci || husky",
		"release": "release-it --ci",
		"start": "next start"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*": [
			"prettier --write --cache --cache-location=.prettiercache --list-different --ignore-unknown",
			"node --experimental-strip-types scripts/update-cspell-words.ts"
		]
	},
	"dependencies": {
		"@ariakit/react": "0.4.16",
		"@sentry/nextjs": "9.11.0",
		"@serwist/next": "9.0.12",
		"blurhash": "2.0.5",
		"clsx": "2.1.1",
		"ky": "1.8.0",
		"motion": "12.6.3",
		"next": "15.2.4",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"serwist": "9.0.12",
		"tailwind-merge": "3.1.0",
		"zod": "3.24.2"
	},
	"devDependencies": {
		"@commitlint/cli": "19.8.0",
		"@commitlint/config-conventional": "19.8.0",
		"@eslint-community/eslint-plugin-eslint-comments": "4.4.1",
		"@eslint-react/eslint-plugin": "1.40.3",
		"@eslint/js": "9.24.0",
		"@ianvs/prettier-plugin-sort-imports": "4.4.1",
		"@inquirer/prompts": "7.4.1",
		"@next/bundle-analyzer": "15.2.4",
		"@next/eslint-plugin-next": "15.2.4",
		"@octokit/core": "6.1.4",
		"@release-it/conventional-changelog": "10.0.0",
		"@tailwindcss/forms": "0.5.10",
		"@tailwindcss/postcss": "4.1.3",
		"@types/fs-extra": "11.0.4",
		"@types/git-url-parse": "16.0.0",
		"@types/node": "22.14.0",
		"@types/react": "19.1.0",
		"@types/react-dom": "19.1.1",
		"@typescript-eslint/parser": "8.29.0",
		"all-contributors-cli": "6.26.1",
		"cross-env": "7.0.3",
		"cspell": "8.18.1",
		"dotenv": "16.4.7",
		"eslint": "9.24.0",
		"eslint-import-resolver-typescript": "4.3.1",
		"eslint-plugin-eslint-comments": "3.2.0",
		"eslint-plugin-import-x": "4.10.0",
		"eslint-plugin-jsdoc": "50.6.9",
		"eslint-plugin-jsonc": "2.20.0",
		"eslint-plugin-jsx-a11y": "6.10.2",
		"eslint-plugin-markdown": "5.1.0",
		"eslint-plugin-n": "17.17.0",
		"eslint-plugin-package-json": "0.29.0",
		"eslint-plugin-perfectionist": "4.11.0",
		"eslint-plugin-react-hooks": "5.2.0",
		"eslint-plugin-react-refresh": "0.4.19",
		"eslint-plugin-regexp": "2.7.0",
		"eslint-plugin-unicorn": "58.0.0",
		"eslint-plugin-yml": "1.17.0",
		"execa": "9.5.2",
		"fs-extra": "11.3.0",
		"git-remote-origin-url": "4.0.0",
		"git-url-parse": "16.0.1",
		"glob": "11.0.1",
		"husky": "9.1.7",
		"is-ci": "4.1.0",
		"knip": "5.46.5",
		"lint-staged": "15.5.0",
		"markdownlint": "0.37.4",
		"markdownlint-cli2": "0.17.2",
		"node-html-parser": "7.0.1",
		"postcss": "8.5.3",
		"prettier": "3.5.3",
		"prettier-plugin-curly": "0.3.2",
		"prettier-plugin-packagejson": "2.5.10",
		"prettier-plugin-sh": "0.16.0",
		"prettier-plugin-tailwindcss": "0.6.11",
		"release-it": "18.1.2",
		"rimraf": "6.0.1",
		"sentences-per-line": "0.3.0",
		"sharp": "0.34.0",
		"source-map-explorer": "2.5.3",
		"stylelint": "16.17.0",
		"stylelint-config-clean-order": "7.0.0",
		"stylelint-config-standard": "37.0.0",
		"svgo": "3.3.2",
		"tailwindcss": "4.1.3",
		"turbo": "2.5.0",
		"typescript": "5.8.3",
		"typescript-eslint": "8.29.0"
	},
	"packageManager": "pnpm@10.7.1",
	"engines": {
		"node": "^20.11.0 || ^22.0.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@sentry/cli",
			"esbuild",
			"sharp"
		],
		"overrides": {
			"@types/react": "19.1.0",
			"@types/react-dom": "19.1.1"
		}
	}
}
