{
	"name": "next-ts-app-template",
	"version": "6.1.0",
	"private": true,
	"description": "Next TS App Template",
	"keywords": [
		"react",
		"typescript",
		"nextjs",
		"tailwindcss"
	],
	"bugs": {
		"url": "https://github.com/timelessco/next-ts-app-template/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/timelessco/next-ts-app-template.git"
	},
	"license": "MIT",
	"author": {
		"name": "Timeless",
		"email": "hello@timeless.co",
		"url": "https://timeless.co/"
	},
	"type": "module",
	"scripts": {
		"build": "next build",
		"build:analyze": "cross-env ANALYZE=true pnpm build && pnpm start",
		"build:ci": "cross-env SKIP_ENV_VALIDATION=true pnpm build",
		"build:icons": "node --experimental-strip-types scripts/build-icons.ts",
		"build:local": "cross-env LOCAL=true pnpm build --turbopack",
		"build:sourcemap": "cross-env SOURCEMAP=true pnpm build && source-map-explorer .next/static/**/*.js && pnpm start",
		"build:start": "pnpm build && pnpm start",
		"check:engine": "npx installed-check --fix",
		"check:packages": "pnpm dedupe --check",
		"check:update": "npx npm-check-updates --interactive --format group --target patch",
		"clean": "node --experimental-strip-types scripts/clean.ts",
		"contributors:add": "all-contributors add",
		"contributors:generate": "all-contributors generate",
		"dev": "next dev --turbopack",
		"dev:sourcemap": "cross-env SOURCEMAP=true pnpm dev",
		"fix": "turbo fix:eslint",
		"fix:css": "stylelint --fix --cache --ignore-path .gitignore --color -f verbose '**/*.css'",
		"fix:eslint": "eslint --cache --color --fix .",
		"fix:md": "markdownlint-cli2 --fix --rules sentences-per-line",
		"fix:prettier": "prettier --write --cache --cache-location=.prettiercache --list-different --ignore-unknown .",
		"fix:spelling": "node --experimental-strip-types scripts/update-cspell-words.ts",
		"lint": "turbo lint:prettier lint:eslint lint:types lint:knip lint:md lint:spelling lint:css",
		"lint:css": "stylelint --cache --ignore-path .gitignore --color -f verbose '**/*.css'",
		"lint:eslint": "eslint --cache --color .",
		"lint:knip": "knip --production",
		"lint:md": "markdownlint-cli2 --rules sentences-per-line",
		"lint:prettier": "prettier --check --cache --cache-location=.prettiercache --ignore-unknown .",
		"lint:spelling": "cspell --dot --gitignore --cache --no-progress --show-context check \"**/*\"",
		"lint:types": "tsc --noEmit",
		"prepare": "is-ci || husky",
		"release": "release-it --ci",
		"start": "next start",
		"sync:template": "node --experimental-strip-types scripts/sync-template-changes.ts"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*": [
			"prettier --write --cache --cache-location=.prettiercache --list-different --ignore-unknown",
			"node --experimental-strip-types scripts/update-cspell-words.ts"
		]
	},
	"dependencies": {
		"@ariakit/react": "0.4.17",
		"@serwist/next": "9.0.15",
		"blurhash": "2.0.5",
		"clsx": "2.1.1",
		"ky": "1.8.1",
		"next": "15.3.4",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"serwist": "9.0.15",
		"tailwind-merge": "3.3.1",
		"zod": "3.25.71"
	},
	"devDependencies": {
		"@commitlint/cli": "19.8.1",
		"@commitlint/config-conventional": "19.8.1",
		"@effect/cli": "0.66.7",
		"@effect/platform": "0.87.7",
		"@effect/platform-node": "0.88.11",
		"@eslint-community/eslint-plugin-eslint-comments": "4.5.0",
		"@eslint-react/eslint-plugin": "1.52.2",
		"@eslint/compat": "1.3.1",
		"@eslint/js": "9.30.1",
		"@ianvs/prettier-plugin-sort-imports": "4.4.2",
		"@inquirer/prompts": "7.6.0",
		"@next/bundle-analyzer": "15.3.4",
		"@next/eslint-plugin-next": "15.3.4",
		"@octokit/core": "7.0.2",
		"@release-it/conventional-changelog": "10.0.1",
		"@sentry/nextjs": "9.34.0",
		"@tailwindcss/forms": "0.5.10",
		"@tailwindcss/postcss": "4.1.11",
		"@types/fs-extra": "11.0.4",
		"@types/node": "24.0.10",
		"@types/react": "19.1.8",
		"@types/react-dom": "19.1.6",
		"@typescript-eslint/parser": "8.35.1",
		"all-contributors-cli": "6.26.1",
		"cross-env": "7.0.3",
		"cspell": "9.1.2",
		"dotenv": "17.0.1",
		"effect": "3.16.12",
		"eslint": "9.30.1",
		"eslint-import-resolver-typescript": "4.4.4",
		"eslint-plugin-eslint-comments": "3.2.0",
		"eslint-plugin-import-x": "4.16.1",
		"eslint-plugin-jsdoc": "51.3.3",
		"eslint-plugin-jsonc": "2.20.1",
		"eslint-plugin-jsx-a11y": "6.10.2",
		"eslint-plugin-n": "17.20.0",
		"eslint-plugin-package-json": "0.43.0",
		"eslint-plugin-perfectionist": "4.15.0",
		"eslint-plugin-react-hooks": "5.2.0",
		"eslint-plugin-react-refresh": "0.4.20",
		"eslint-plugin-regexp": "2.9.0",
		"eslint-plugin-unicorn": "59.0.1",
		"eslint-plugin-yml": "1.18.0",
		"execa": "9.6.0",
		"fs-extra": "11.3.0",
		"git-remote-origin-url": "4.0.0",
		"git-url-parse": "16.1.0",
		"glob": "11.0.3",
		"husky": "9.1.7",
		"is-ci": "4.1.0",
		"knip": "5.61.3",
		"lint-staged": "16.1.2",
		"markdownlint": "0.38.0",
		"markdownlint-cli2": "0.18.1",
		"node-html-parser": "7.0.1",
		"postcss": "8.5.6",
		"prettier": "3.6.2",
		"prettier-plugin-curly": "0.3.2",
		"prettier-plugin-packagejson": "2.5.17",
		"prettier-plugin-sh": "0.18.0",
		"prettier-plugin-tailwindcss": "0.6.13",
		"release-it": "19.0.3",
		"sentences-per-line": "0.3.0",
		"sharp": "0.34.2",
		"source-map-explorer": "2.5.3",
		"stylelint": "16.21.1",
		"stylelint-config-clean-order": "7.0.0",
		"stylelint-config-standard": "38.0.0",
		"svgo": "4.0.0",
		"tailwindcss": "4.1.11",
		"turbo": "2.5.4",
		"typescript": "5.8.3",
		"typescript-eslint": "8.35.1"
	},
	"packageManager": "pnpm@10.12.4",
	"engines": {
		"node": "^20.19.0 || ^22.12.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@sentry/cli",
			"@tailwindcss/oxide",
			"esbuild",
			"sharp",
			"unrs-resolver"
		],
		"overrides": {
			"@types/react": "19.1.8",
			"@types/react-dom": "19.1.6"
		}
	}
}
